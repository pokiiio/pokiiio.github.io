          <guid isPermalink="false">hatenablog://entry/10257846132693385141</guid>
          
          <enclosure url="https://lh3.googleusercontent.com/OwvY8vWSRSZhK6k4X-u4xNSe5-Y_japUbqenajTQublkdwDbowmLlo6sNziCJBYwNU_kKLQIHd1fVpes2DCgRkBh14M8VAjBplHmRjkyFhzewy3ngLzwERbWUPuFY1VoTLTm02gMpco=s600" type="" length="0" />
