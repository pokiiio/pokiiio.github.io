          <guid isPermalink="false">hatenablog://entry/10257846132638840181</guid>
          
          <enclosure url="https://lh3.googleusercontent.com/U4qMamyv_TKH1xPdDilcGyYQeSUyjMGUMbDZl2X9nuoxMfN8trqLoNEiv9u1T33osCALcit4ISz6xl5BQbWpMv_2V4unWBkKLrZnMSZjvUz8iEK8SsgHpAM7vdbqyGyALDWHIkjXDo4=s600" type="" length="0" />
